// Write a function that displays numbers from given start to given end and their sum. The input comes as two number parameters.


function printSum (numStart, numEnd) {

    let nums = "";
    let sum = 0;

for (let i = numStart; i <=numEnd; i++) {
    nums += i + " ";
    sum = sum +i;
}

console.log (nums);
console.log (`Sum: ${sum}`);

} 
